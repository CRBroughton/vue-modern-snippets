{
  "name": "modern-vue-snippets",
  "displayName": "Modern Vue Snippets",
  "description": "Modern Vue Snippets",
  "version": "0.0.1",
  "publisher": "CraigRBroughton",
  "engines": {
    "vscode": "^1.95.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CRBroughton/vue-vscode-snippets.git"
  },
  "scripts": {
    "build": "vsce package -o vue-snippets-$npm_package_version.vsix",
    "install": "npm run build && code --install-extension vue-snippets-$npm_package_version.vsix",
    "changeset": "npx changeset",
    "changeset:status": "npx changeset status --verbose",
    "changeset:version": "npx changeset version"
  },
  "keywords": [
    "Vue",
    "Nuxt",
    "Vue 3",
    "Composition API",
    "Pinia",
    "Vue Snippets"
  ],
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "vue",
        "path": "./snippets/vue.json"
      },
      {
        "language": "html",
        "path": "./snippets/vue-template.json"
      },
      {
        "language": "vue-html",
        "path": "./snippets/vue-template.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/vue-script.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/nuxt-template.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/nuxt-script.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/pinia.json"
      }
    ]
  }
}
